<Window x:Class="SevenWonders.changeNicknameUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Change Nickname" Height="126" Width="300" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" WindowStartupLocation="CenterScreen">
    <Grid Height="193" Width="276">
        <Grid.RowDefinitions>
            <RowDefinition Height="240*" />
            <RowDefinition Height="28*" />
        </Grid.RowDefinitions>
        <Label Content="New Nickname" Height="23" HorizontalAlignment="Left" Margin="10,8,0,0" Name="label1" VerticalAlignment="Top" Width="98" />
        <TextBox Height="26" HorizontalAlignment="Left" Margin="112,8,0,0" Name="nickText" VerticalAlignment="Top" Width="156" />
        <Button Content="OK" Height="25" HorizontalAlignment="Left" Margin="167,50,0,0" Name="button1" VerticalAlignment="Top" Width="42" Click="button1_Click" />
        <Button Content="Cancel" Height="25" HorizontalAlignment="Left" Margin="215,50,0,0" Name="button2" VerticalAlignment="Top" Width="51" />
    </Grid>
</Window>
