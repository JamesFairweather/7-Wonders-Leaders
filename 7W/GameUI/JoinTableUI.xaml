<Window x:Class="SevenWonders.JoinTableUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Join Table" Height="152" Width="289" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <ImageBrush />
    </Window.Background>
    <Grid Width="275" Height="117">
        <Grid.Background>
            <ImageBrush ImageSource="/7W;component/bin/Debug/Images/background.jpg" />
        </Grid.Background>
        <TextBox Height="28" HorizontalAlignment="Left" Margin="92,12,0,0" Name="ipAddressText" VerticalAlignment="Top" Width="172" Text="" />
        <Label Content="Server IP" Height="28" Name="label1" VerticalAlignment="Top" Margin="12,10,0,0" HorizontalAlignment="Left" Width="62" />
        <Button Height="23" HorizontalAlignment="Left" Margin="92,82,0,0" Name="joinButton" VerticalAlignment="Top" Width="89" Click="button1_Click" Content="Join" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="189,82,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click"></Button>
        <Label Content="Nick Name" Height="30" HorizontalAlignment="Left" Margin="12,42,0,0" Name="label2" VerticalAlignment="Top" Width="74" />
        <TextBox Height="28" HorizontalAlignment="Left" Margin="92,44,0,0" Name="textUser" VerticalAlignment="Top" Width="172" Text="" />
    </Grid>
</Window>
